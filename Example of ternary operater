package MyProject;

public abstract class Ternary {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
        int b = 100;
        int c = 300;
        
        String ter1 = ((a>b)&&(a>c))?(a+": is largest no"):(b>c)?(b+": is largest no"):(c+": is largest no");
        System.out.print(ter1);

	}

}
